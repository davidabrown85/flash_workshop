package classes{	import flash.display.MovieClip;	import flash.net.URLLoader;	import flash.net.URLRequest;	import flash.events.Event;		public class TwitterDemo extends MovieClip	{		public function TwitterDemo()		{			loadTweats();		}						public function loadTweats():void		{			// https://dev.twitter.com/docs/api/1/get/statuses/user_timeline					var myXMLLoader:URLLoader = new URLLoader(); 			//myXMLLoader.load( new URLRequest( "http://twitter.com/statuses/public_timeline.xml" ) );			myXMLLoader.load( new URLRequest( "http://twitter.com/statuses/user_timeline.xml?screen_name=DavidABrown70" ) );			myXMLLoader.addEventListener( Event.COMPLETE, processXML ); 		}						public function processXML( ev:Event ):void		{ 			var myXML:XML = new XML( ev.target.data );						trace( myXML.status[ 0 ] );						//trace( myXML.status[ 0 ].text );			//trace( myXML.status[ 0 ].name );		}	}}