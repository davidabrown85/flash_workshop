package  {		import flash.display.MovieClip;	import classes.DigitalTimerCountdown;			public class MousePointer extends MovieClip {						public function MousePointer() {			//timer_mc.setup_timer( 10000 );			//timer_mc.start_timer();						var clock:DigitalTimerCountdown = new DigitalTimerCountdown();			clock.setup_timer( 10000 );			clock.start_timer();			clock.x = 100;			clock.y = 100;						addChild(clock);		}	}	}