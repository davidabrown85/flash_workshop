package classes{	import flash.display.MovieClip;		import flash.media.Video;	import flash.media.Camera;	public class CameraDemo extends MovieClip	{		public function CameraDemo()		{			init();		}				private function init():void		{			var video:Video = new Video( 550, 400 );						var camara:Camera = Camera.getCamera( );						camara.setMode( 500, 400, 30 );						video.attachCamera( camara );						addChild( video );		}	}}