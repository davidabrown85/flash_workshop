package classes{	import flash.display.MovieClip;	import flash.printing.PrintJob;	import flash.events.MouseEvent;	public class PrintDemo extends MovieClip	{		public function PrintDemo()		{			print_btn.addEventListener( MouseEvent.MOUSE_UP, print_pushed );		}				private function print_pushed( ev:MouseEvent ):void		{			print_btn.removeEventListener( MouseEvent.MOUSE_UP, print_pushed );			print_movieclip( example_image );					}				public function print_movieclip( mc:MovieClip ):void		{			var init_w:int = mc.width;			var init_h:int = mc.height;						var printJob:PrintJob = new PrintJob();			if ( printJob.start() ) 			{				if ( mc.width > printJob.pageWidth ) 				{					mc.width = printJob.pageWidth;					mc.scaleY = mc.scaleX;				}						printJob.addPage( mc );				printJob.send();			}						mc.width = init_w;			mc.height = init_h;					}	}}