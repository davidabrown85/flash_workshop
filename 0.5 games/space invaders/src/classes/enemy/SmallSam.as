package classes{	import flash.display.MovieClip;	import flash.events.Event;	public class SmallSam extends Enemy	{		public function SmallSam()		{			speed = 5;		}				public override function update( ev:Event ):void		{			y += speed;			rotation +=1;						if( y > stage.stageHeight )			{								removeEventListener( Event.ENTER_FRAME, update );				dispatchEvent( new Event("finished" ) );			}		}	}}