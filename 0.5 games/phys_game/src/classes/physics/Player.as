package classes.physics{		import flash.display.MovieClip;		import Box2D.Collision.Shapes.b2CircleShape;	import Box2D.Dynamics.b2FixtureDef;	import Box2D.Dynamics.b2BodyDef;	import Box2D.Dynamics.b2Body;	import flash.events.Event;			public class Player extends Physics_Object	{		public var body:b2Body;				public function Player():void 		{			object_type = "player_mc";		}						public function create_object( ):void		{			var circle_shape:b2CircleShape = setup_manual_circle( 40 );									var phys_properties:b2FixtureDef = new b2FixtureDef();					phys_properties.shape = circle_shape;				phys_properties.density = 0.5;			phys_properties.friction = .2;			phys_properties.restitution = 0;			//phys_properties.filter.categoryBits = layer_array[ which_layer - 1 ];			//phys_properties.filter.maskBits = layer_array[ which_layer - 1 ];			//phys_properties.filter.groupIndex = which_layer;												var bodyDef:b2BodyDef = setup_body( this );			//bodyDef.linearDamping = 0; 			//bodyDef.angularDamping = 5;			bodyDef.type = b2Body.b2_dynamicBody;			body = physics_world.CreateBody( bodyDef );			body.CreateFixture( phys_properties );			body.SetFixedRotation( true );			//body.IsBullet();						var graphicMC:MovieClip = create_graphic( this );			graphicMC.name = object_type;									var object_data:Object = new Object();			object_data.body = body;			object_data.texture = graphicMC;			object_data.status = "";			object_data.name = object_type;			object_data.x = x;			object_data.y = y;						object_data.type = CallbackTypes.PLAYER;						body.SetUserData( object_data );						clear_movieclip( this );						addChild( object_data.texture );		} 	}}