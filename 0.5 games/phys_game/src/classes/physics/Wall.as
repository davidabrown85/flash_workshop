package classes.physics{		import flash.display.MovieClip;		import Box2D.Collision.Shapes.b2PolygonShape;	import Box2D.Dynamics.b2FixtureDef;	import Box2D.Dynamics.b2BodyDef;	import Box2D.Dynamics.b2Body; 			public class Wall extends Physics_Object	{				public function Wall():void {  }						public function create_object( ):void		{						var default_r:Number = rotation;			rotation = 0;						var boxDef:b2PolygonShape = setup_box( this );												var phys_properties:b2FixtureDef = new b2FixtureDef();						phys_properties.shape = boxDef;					//phys_properties.density = 1;			phys_properties.friction = 2;			phys_properties.restitution = 0;						//phys_properties.filter.categoryBits = layer_array[ which_layer - 1 ];			//phys_properties.filter.maskBits = layer_array[ which_layer - 1 ];			//phys_properties.filter.groupIndex = which_layer;												var bodyDef:b2BodyDef = setup_body( this );			bodyDef.type = b2Body.b2_staticBody;						var body:b2Body = physics_world.CreateBody( bodyDef );			body.CreateFixture( phys_properties );			body.SetAngle( default_r / (180 / Math.PI) );						var object_data:Object = new Object();			object_data.body = body;			object_data.texture = null;			object_data.status = "";			object_data.name = object_type;			object_data.jumpable = "true";			object_data.startX = x;			object_data.startY = y;			object_data.type = CallbackTypes.GROUND;						body.SetUserData( object_data );						clear_movieclip( this );		}	}}