package classes.LevelObjects{		import flash.display.MovieClip;		import Box2D.Collision.Shapes.b2CircleShape;	import Box2D.Dynamics.b2FixtureDef;	import Box2D.Dynamics.b2BodyDef;	import Box2D.Dynamics.b2Body;	import flash.events.Event;			public class Foot_Ball extends Physics_Object	{				public function Foot_Ball():void 		{			object_type = "foot_ball_mc";		}						public function create_object( ):Object		{						var graphicMC:MovieClip = new ball_mc();			graphicMC.name = "foot_ball";						var circleDef:b2CircleShape = setup_circle( this );						var fixtureDef:b2FixtureDef = new b2FixtureDef();						fixtureDef.shape = circleDef;									fixtureDef.density = 1.0;			fixtureDef.friction = 1;			fixtureDef.restitution = .5;			fixtureDef.filter.categoryBits = 2;				fixtureDef.filter.maskBits = 2;			fixtureDef.userData = graphicMC;			fixtureDef.userData.width = width * 1.2;			fixtureDef.userData.height = height * 1.2;									var bodyDef:b2BodyDef = setup_body( this );			//bodyDef.linearDamping = 0;			//bodyDef.angularDamping = 5;			bodyDef.type = b2Body.b2_dynamicBody;			var body:b2Body = physics_world.CreateBody( bodyDef );			body.CreateFixture( fixtureDef );						clear_movieclip();						addChild( fixtureDef.userData );									var ball_object:Object = new Object();			ball_object.fixture = fixtureDef;			ball_object.body = body;									return ball_object;		}				private function clear_movieclip():void		{			x = y = 0;			for( var i:int = 0; i < numChildren; i++ )			{				removeChildAt( 0 );			}		}		 	}}