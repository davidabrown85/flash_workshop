package classes.LevelObjects{		import flash.display.MovieClip;		import Box2D.Collision.Shapes.b2PolygonShape;	import Box2D.Dynamics.b2FixtureDef;	import Box2D.Dynamics.b2BodyDef;	import Box2D.Dynamics.b2Body; 			public class Wall extends Physics_Object	{				public function Wall():void {  }						public function create_object( ):void		{						var boxDef:b2PolygonShape = setup_box( this );												var fixtureDef:b2FixtureDef = new b2FixtureDef();						fixtureDef.shape = boxDef;									fixtureDef.friction = .1;			fixtureDef.restitution = .1;			fixtureDef.filter.categoryBits = 2;				fixtureDef.userData = new MovieClip();									var bodyDef:b2BodyDef = setup_body( this );			bodyDef.type = b2Body.b2_staticBody;						var body:b2Body = physics_world.CreateBody( bodyDef );			body.CreateFixture( fixtureDef );						clear_movieclip();		}						private function clear_movieclip():void		{			x = y = 0;			for( var i:int = 0; i < numChildren; i++ )			{				removeChildAt( 0 );			}		}	}}